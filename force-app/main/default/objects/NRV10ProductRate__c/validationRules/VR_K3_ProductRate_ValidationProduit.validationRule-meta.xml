<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR_K3_ProductRate_ValidationProduit</fullName>
    <active>true</active>
    <description>Vérifie que le produit est renseigné</description>
    <errorConditionFormula>AND(!$User.ByPassValidationRule__c,ISBLANK(product__c))</errorConditionFormula>
    <errorDisplayField>product__c</errorDisplayField>
    <errorMessage>Le champs produit doit être renseigné</errorMessage>
</ValidationRule>
