<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR_K3_ProductRate_ValidationTaux</fullName>
    <active>true</active>
    <description>Vérifie que le taux est renseigné</description>
    <errorConditionFormula>AND(!$User.ByPassValidationRule__c, ISBLANK(salesrate__c))</errorConditionFormula>
    <errorDisplayField>salesrate__c</errorDisplayField>
    <errorMessage>Le champs Taux de vente doit être renseigné</errorMessage>
</ValidationRule>
