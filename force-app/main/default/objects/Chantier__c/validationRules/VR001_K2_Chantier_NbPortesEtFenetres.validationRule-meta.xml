<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR001_K2_Chantier_NbPortesEtFenetres</fullName>
    <active>true</active>
    <errorConditionFormula>$User.ByPassValidationRule__c == FALSE &amp;&amp; !ISNULL( nbPortesEtFenetres__c ) &amp;&amp; nbPortesEtFenetres__c &lt; 0</errorConditionFormula>
