<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR_K3_ESCIncentive_Amount</fullName>
    <active>true</active>
    <errorConditionFormula>$User.ByPassValidationRule__c = false &amp;&amp; customerAmount__c &lt; 0 &amp;&amp; customerAmount__c &lt;= officialAmount__c</errorConditionFormula>
    <errorDisplayField>customerAmount__c</errorDisplayField>
    <errorMessage>Montant de la prime CEE invalide (doit être supérieur ou égal à 0 et inférieur ou égal au montant officiel)!</errorMessage>
</ValidationRule>
