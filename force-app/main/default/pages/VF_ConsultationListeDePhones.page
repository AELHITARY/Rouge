        function selectedCheckboxes(type){
            if($j("."+selectedClass).length == 0) // Vérifie les lignes sélectionnées
                var lines = $j("[id^=tableLine]"); // Récupération de toutes les lignes du tableau
            else
                var lines = $j("."+selectedClass); // Récupération seulement des lignes sélectionnées
            // Detect IE
            var ua = window.navigator.userAgent;
            var msie = ua.indexOf("MSIE ");
            lines.each(function( index ) {
                var appelBox = this.children[8].children[0];
                var arguBox = this.children[9].children[0];
                if(type == 'appel'){
