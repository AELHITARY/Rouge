                        eventFire(appelBox, 'click');
                    } else if(!$j("[id$=appel_all]")[0].checked && $j(appelBox).is(':checked')){
                        if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) 
                            $j(appelBox).checked = false;
                        else
                            $j(appelBox).prop("checked", false);
                        eventFire(appelBox, 'click');
                    }
